                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select x.cont,f.MARCA, f.NUME,f.PRENUME,f.CNP, MAX(f.ANLUNA) as ANLUNA,MAX(f.cont_banca) as CONT_BANCA
from (
   select  MID(cont_banca,5,4) as cont,cont_banca, MAX(MARCA) as MARCA,MAX(ANLUNA)as ANLUNA
   from PERS 
   group by cont_banca 
   
) as x 
inner join PERS as f 
on f.cont_banca = x.cont_banca and f.ANLUNA = x.ANLUNA and f.MARCA = x.MARCA
WHERE f.cont_banca like '%BT%' OR  f.cont_banca like '%BPOS%'  OR  f.cont_banca like '%RNCB%' 
group by x.cont,f.MARCA,f.NUME,f.PRENUME,f.CNP
order by Nume,Prenume</CommandText>
